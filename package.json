{
  "name": "@jpereraaracil/rqts",
  "version": "7.2.0",
  "description": "Rx Queue in TypeScript",
  "main": "lib/bundle.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf lib && bun run cleanBuild",
    "cleanBuild": "rm -rf build",
    "lint": "eslint --config .eslintrc.js --fix ./src ",
    "build:tsc": "tsc",
    "build:webpack": "webpack --mode production --config webpack.config.js",
    "build:webpack:dev": "webpack --mode development --config webpack.config.js",
    "prebuild": "bun run clean",
    "build": "bun run build:tsc && bun run build:webpack && bun run cleanBuild",
    "test": "jest",
    "postinstall": "bun run build",
    "prepublishOnly": "bun run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesuspereraaracil/RQTS.git"
  },
  "keywords": [
    "rx",
    "typescript",
    "reactive",
    "queue"
  ],
  "author": "jesuspereraaracil",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/jesuspereraaracil/RQTS/issues"
  },
  "homepage": "https://github.com/jesuspereraaracil/RQTS#readme",
  "dependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-typescript": "^7.23.3",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.100",
    "@types/bun": "^1.0.0",
    "@types/jest": "^29.5.10",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "babel-loader": "^9.1.3",
    "bun": "^1.0.18",
    "bun-types": "^1.0.18",
    "dts-loader": "^0.1.8",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^1.0.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
